{
  "name": "@cycjimmy/hello-github-package-npm",
  "version": "1.1.0",
  "description": "hello github package npm",
  "main": "dist/hello-world.cjs.js",
  "module": "dist/hello-world.esm.js",
  "browser": "dist/hello-world.umd.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint -c .eslintrc 'src/**/*.js'",
    "lint:fix": "eslint --fix -c .eslintrc 'src/**/*.js' --fix",
    "dev": "rollup -c rollup/rollup.config.dev.js --watch",
    "build": "cross-env NODE_ENV=production rollup -c rollup/rollup.config.prod.js",
    "build:dev": "cross-env NODE_ENV=production rollup -c rollup/rollup.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cycjimmy/hello-github-package-npm.git"
  },
  "keywords": [
    "github",
    "package",
    "npm"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycjimmy/hello-github-package-npm/issues"
  },
  "homepage": "https://github.com/cycjimmy/hello-github-package-npm#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@cycjimmy/config-lib": "^1.3.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-rollup": "^0.1.0",
    "jest": "^24.9.0",
    "rollup": "^1.25.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-browsersync": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
